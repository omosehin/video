@model Vidly.ViewModels.ViewCustomerModel

@{
    ViewBag.Title = "Customers"; //title of our page shown in the browser
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@ViewBag.Title</h2>

<div>
    <ul>
        @foreach (var customer in Model.Customers)
        {
            <li>@customer.Name</li>
        }
    </ul>
</div>